.col-md-12.no_padd{:style=>"widht: 100%"}
  .navbar-header
    .row.bord_bott
      .navbar-toggle{ :style=>"float: left; padding: 0px; margin: 0px; width: 100%;"}
        .row
          .col-xs-1.no_padd{:style=>"cursor: pointer"}
            .padd_left_md
              .close-icon{:style=>"padding-top: 14pt; display: none; margin-left: -2px", "ng-click"=> "hideXsMenu()"}
                =image_tag "new/icons/close.svg", :width=>"25pt", :style=>"opacity: 0.7"
              .menu-icon{:style=>"padding-top: 18pt", "data-target" => "#myNavbar", "data-toggle" => "collapse", :type => "button", "ng-click"=> "showXsMenu()"}
                %span.icon-bar
                %span.icon-bar
                %span.icon-bar
          .col-xs-10.no_padd{:style=>"text-align: left;"}
            %h3.step_text.side_menu_h{:style=>"color: #434343"}
              Workspace
  #target.container-fluid.no_padd.no_marg
    #myNavbar.collapse.navbar-collapse.no_padd.no_marg
      %ul.nav.navbar-nav.no_padd.no_marg{:style=>"float: none; width: 100%"}
        #cluster_actions_drop.bord_bottg.padd_bott_md.install_step.side_menu_element
          .flex-container-all
            .flex-wrap
              %h3.left-menu-h.no_marg_bott CLUSTER
              .padd_left_md
                .div{:width=>50, :style=>"border-radius: 0px; border: 0px solid rgba(212, 212, 212, 0.6);"}
                  .fl-cont.fl-h-center
                    .fl-wrap{style:'margin-top: 1px;'}
                      .div(ng-if="currentCluster.clusterType == 'aws' ")
                        =image_tag "new/icons/aws.svg", class: "workspace_icon workspace_icon_", style:"width: 13px; padding-right: 0px;"
                      .div(ng-if="currentCluster.clusterType == 'onprem' || currentCluster.clusterType == '' ")
                        =image_tag "new/icons/dns.svg", class: "workspace_icon workspace_icon_", style:"width: 13px; padding-right: 0px;"
                    .fl-wrap
                      %h6.no_marg.inl.gr{:style=>"font-weight: 400; font-size: 12pt;"}
                        -#=image_tag "new/icons/shares_.svg", "ng-if":"currentCluster.team.name != currentTeam.name", class: "workspace_icon workspace_icon_", style:"width: 16px; padding-right: 0px;"
                        {{currentCluster.name}}

            .flex-wrap{style:" flex-grow: 1;"}
              .pull-right
                =image_tag "new/down.png", :class=>"inl", id:"demo-menu-lower-left",:style=>"opacity: 0.6; width: 15pt; margin: 22px 20px 0px 20px"



        %a(data-div="stats-tab" ng-href="{{ROUTES.getAddr('CLUSTER_STATISTICS', currentCluster.id)}}" class="left-menu-el" ng-class="pageSelected == 'stats' ? 'left-menu-current' : ''")
          .fl-cont.gx-fl-center
            .fl-wrap.padd_ri_sm
              = inline_svg "new/icons/statistics_.svg"
            .fl-wrap
              %h3 Statistics

        %a(data-div="nodes-tab" ng-href="{{ROUTES.getAddr('NODES', currentCluster.id)}}" class="left-menu-el" ng-class="pageSelected == 'nodes' ? 'left-menu-current' : ''")
          .fl-cont.gx-fl-center
            .fl-wrap.padd_ri_sm
              = inline_svg "new/icons/nodes_.svg"
            .fl-wrap
              %h3 Nodes

        %a(data-div="containers-tab" ng-href="{{ROUTES.getAddr('CONTAINERS', currentCluster.id)}}" class="left-menu-el" ng-class="pageSelected == 'containers' ? 'left-menu-current' : ''")
          .fl-cont.gx-fl-center
            .fl-wrap.padd_ri_sm
              = inline_svg "new/icons/cont_.svg"
            .fl-wrap
              %h3 Containers

        -#%a(ng-href="{{ROUTES.getAddr('CLUSTER_STATISTICS', currentCluster.id)}}" class="undec")
          .install_step.side_menu_element(data-div="stats-tab" ng-class="pageSelected == 'stats' ? 'current_step' : ''")
            %h3.step_text.step_text_icon.side_menu_text.no_marg_top(ng-class="pageSelected == 'stats' ? 'current_step_text' : ''")
              = inline_svg "new/icons/statistics_.svg", class: "workspace_icon {{pageSelected == 'stats' ? 'workspace_icon_current' : ''}}"
              Statistics

        -#%a(ng-href="{{ROUTES.getAddr('NODES', currentCluster.id)}}" class="undec")
          .install_step.side_menu_element(data-div="nodes-tab" ng-class="pageSelected == 'nodes' ? 'current_step' : ''")
            %h3.step_text.step_text_icon.side_menu_text(ng-class="pageSelected == 'nodes' ? 'current_step_text' : ''")
              = inline_svg "new/icons/nodes_.svg", class: "workspace_icon {{pageSelected == 'nodes' ? 'workspace_icon_current' : ''}}"
              Nodes

        -#%a(ng-href="{{ROUTES.getAddr('CONTAINERS', currentCluster.id)}}" class="undec")
          .install_step.side_menu_element(data-div="containers-tab" ng-class="pageSelected == 'containers' ? 'current_step' : ''")
            %h3.step_text.step_text_icon.side_menu_text(ng-class="pageSelected == 'containers' ? 'current_step_text' : ''")
              = inline_svg "new/icons/cont_.svg", class: "workspace_icon {{pageSelected == 'containers' ? 'workspace_icon_current' : ''}}"
              Containers


        -#.bord_bott.padd_top_10{style:'margin: 0 30px;'}
        -#.padd_top_sm

        %a(data-div="shares-tab" ng-if="userPermissions.shares" ng-href="{{ROUTES.getAddr('SHARES', currentCluster.id)}}" class="left-menu-el" ng-class="pageSelected == 'shares' ? 'left-menu-current' : ''")
          .fl-cont.gx-fl-center
            .fl-wrap.padd_ri_sm
              = inline_svg "new/icons/shares_.svg"
            .fl-wrap
              %h3 Shares

        %a(data-div="logs-tab" ng-if="userPermissions.logs" ng-href="{{ROUTES.getAddr('LOGS', currentCluster.id)}}" class="left-menu-el" ng-class="pageSelected == 'logs' ? 'left-menu-current' : ''")
          .fl-cont.gx-fl-center
            .fl-wrap.padd_ri_sm
              = inline_svg "new/icons/logs_.svg"
            .fl-wrap
              %h3 Logs


        .bord_bott.padd_top_10{style:'margin: 0 30px;'}
        .padd_top_sm


        %a(data-div="big-data-tab" ng-href="{{ROUTES.getAddr('HDP_APP_SERVICES_BROWSER', currentCluster.id, hadoopApp.id)}}" class="left-menu-el" ng-class="pageSelected == 'hadoop' ? 'left-menu-current' : ''")
          .fl-cont.gx-fl-center
            .fl-wrap.padd_ri_sm
              -#=image_tag "new/icons/hadoop_logo.png", width: 24
              = inline_svg "new/icons/components.svg"
            .fl-wrap
              %h3 Big Data

        %a(data-div="vis-tab" ng-href="{{ROUTES.getAddr('APPLICATION_SERVICES_VIS_BROWSER', currentCluster.id, hadoopApp.id)}}" class="left-menu-el" ng-class="pageSelected == 'visualize' ? 'left-menu-current' : ''")
          .fl-cont.gx-fl-center
            .fl-wrap.padd_ri_sm
              = inline_svg "new/icons/bubles.svg"
            .fl-wrap
              %h3 Search/Visualize


        %a(data-div="trans-tab" ng-href="{{ROUTES.getAddr('APPLICATION_SERVICES_TRANS_BROWSER', currentCluster.id, hadoopApp.id)}}" class="left-menu-el" ng-class="pageSelected == 'transform' ? 'left-menu-current' : ''")
          .fl-cont.gx-fl-center
            .fl-wrap.padd_ri_sm
              = inline_svg "new/icons/transform.svg"
            .fl-wrap
              %h3 Transform

        .bord_bott.padd_top_10{style:'margin: 0 30px;'}
        .padd_top_sm

        %a(data-div="apphub-tab" ng-href="{{ROUTES.getAddr('LIBRARY_APP_LIST', currentCluster.id)}}" class="left-menu-el" ng-class="pageSelected == 'libApps' ? 'left-menu-current' : ''")
          .fl-cont.gx-fl-center
            .fl-wrap.padd_ri_sm
              = inline_svg "new/icons/mall.svg"
            .fl-wrap
              %h3 AppHubâ„¢


        %a(data-div="installed-apps-tab" ng-href="{{ROUTES.getAddr('APPLICATIONS', currentCluster.id)}}" class="left-menu-el" ng-class="pageSelected == 'apps' ? 'left-menu-current' : ''")
          .fl-cont.gx-fl-center
            .fl-wrap.padd_ri_sm
              = inline_svg "new/icons/ticket.svg"
            .fl-wrap
              %h3 Installed Apps



        %a(data-div="app-{{app.name}}-tab" ng-repeat="app in apps track by app.id" ng-href="{{ROUTES.getAddr('APPLICATION_SERVICES_BROWSER', currentCluster.id, app.id)}}" class="undec")
          .div(class="left-menu-el" ng-class="pageSelected == ('app_' + app.id) ? 'left-menu-current' : ''")
            .fl-cont.gx-fl-center
              .fl-wrap.padd_ri_sm
                %img(ng-if="!app.external" ng-src="{{app.icon_url ? app.icon_url : app.image_url}}"){style:'width: 24px; border-radius: 4px;'}
                = inline_svg "new/icons/extention.svg", :ng_if=> "app.external", width: "24px", height: "24px"
              .fl-wrap
                %h3 {{app.title ? app.title : app.name}}

